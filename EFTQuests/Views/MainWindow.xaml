<Window x:Class="EFTQuests.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="EFTQuests" 
        xmlns:viewModels="clr-namespace:EFTQuests.ViewModels"
        ResizeMode="CanResizeWithGrip" 
        MinWidth="800" 
        MinHeight="600" 
        Background="Black"
        d:DataContext="{d:DesignInstance viewModels:MainWindowViewModel}">
<Grid Name="MainGrid">
    <StackPanel>
    <Button Name="CreateSheet" Height="20" Width="100" VerticalAlignment="Top" HorizontalAlignment="Center" Margin="0,50,0,0"  >Create</Button>
    <ListBox Margin="0,20,0,0" MinWidth="100" MinHeight="100" Background="Black" HorizontalContentAlignment="Center">
        <ListBoxItem Foreground="White"> sheet1</ListBoxItem>
        <ListBoxItem Foreground="White">sheet2</ListBoxItem>
        <ListBoxItem Foreground="White">sheet2</ListBoxItem>
    </ListBox>
    </StackPanel>
</Grid>
</Window>
